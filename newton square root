def newtonSqrt(n, base):
    approx_root = 0.5 * n
    for i in range(base):
        betterapprox = 0.5 * (approx_root + n/approx_root)
        approx_root = betterapprox
    return betterapprox
print("the square root of 70 :",newtonSqrt(70, 10))
print("the square root of 60 :",newtonSqrt(60, 10))
print("the square root of 80:",newtonSqrt(80, 10))
