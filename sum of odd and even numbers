def sum_odd_and_even_numbers(numbers):
    odd_sum = 0
    even_sum = 0

    for number in numbers:
        if number % 2 == 0:
            even_sum +=1
        else:
            odd_sum +=1 

    return odd_sum, even_sum
my_list = [2,6,1,3,11,13,4,6,14,15]
odd_sum, even_sum = sum_odd_and_even_numbers(my_list)
print("Sum of odd :",odd_sum)
print("Sum of even:",even_sum)
